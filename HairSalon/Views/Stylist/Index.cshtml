@{
Layout = "_Layout";
}
<div class="container">
<h1>Employee Stylist</h1>
<hr />


    @foreach (var item in Model)
    {
      <li><a href="stylists/@item.GetId()"> @item.GetName()</a></li>

    }


<p><a href="/stylists/new" class="btn btn-info">Add a new Hire</a></p>
<hr>
<form action="stylist/delete" method="post">
<button type="submit" class="btn btn-info">Delete</button>
<div class="btn btn-info"><a href="/">Go back</a></div>
</form>
</div>