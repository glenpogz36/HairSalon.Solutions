@{
Layout = "_Layout";
}
<h3>Here are all the clients for you:</h3>
<ol>
@foreach (var client in @Model["client"])
{
  <li>@client.GetClientName()</li>
}
</ol>


<p><a  class="btn btn-warning" href='/stylists/new'>Add another stylist</a></p>
<p><a  class="btn btn-info" href='/stylists'>View all Stylists</a></p>
<p><a  class="btn btn-primary" href='/stylists/@Model["stylist"].GetId()/clients/new'>Add a client</a></p>
<p><a  class="btn btn-warning" href='/stylists/@Model["stylist"].GetId()/delete'>Clear all Cients</a></p>